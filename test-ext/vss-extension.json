{
  "manifestVersion": 1,
  "version": "1.200.71",
  "publisher": "jessehouwing",
  "id": "test-extension",
  "name": "Test Extension",
  "description": "A test extension for validation",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Utility",
    "test"
  ],
  "targets": [
    {
      "id": "Microsoft.TeamFoundation.Server",
      "version": "[16.0, 20.0)"
    }
  ],
  "icons": {
    "default": "icon-default.png",
    "large": "icon-large.png"
  },
  "content": {
    "details": {
      "path": "overview.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "files": [
    {
      "path": "_tasks"
    }
  ],
  "contributions": [
    {
      "id": "TestTask-sxs",
      "type": "ms.vss-distributed-task.task",
      "targets": ["ms.vss-distributed-task.tasks"],
      "properties": {
        "name": "_tasks/TestTask-sxs"
      }
    }
  ]
}